{
    "name": "shmit/slim-swoole",
    "type": "project",
    "description": "Slim with Swoole boilerplate project.",
    "keywords": [
        "boilerplate",
        "slim",
        "swoole"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "slim/slim": "^3.0",
        "monolog/monolog": "^1.24",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
